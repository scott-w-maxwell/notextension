{
  "name": "noextension",
  "description": "A notepad for your web browser",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons":{
      "128":"icon.png"
    },
  "permissions":[
    "history",
    "storage"
  ]
}